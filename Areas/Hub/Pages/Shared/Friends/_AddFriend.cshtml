@using LetsGame.Areas.Identity.Data
@using LetsGame.Services
@using LetsGame.Data.Models
@model List<FriendData>

<form id="page-form" type="post" asp-area="Hub" asp-page="Friends/Index">
    @*<div id="search-results"></div>*@
    @*<input type="text" size="30" onkeyup="displayUsers(this.value)" />*@
    <input id="selection" list="users" onkeyup="displayUsers(this.value)">
    <input id="id-value" type="hidden" name="friendID" />
    <div id="search-results">
        <partial name="_UserSearchResults" model="@Model" />
    </div>
    <button type="submit" onclick="submitFriendRequest()" asp-page-handler="AddFriend" class="btn btn-danger">Add Friend</button>
</form>